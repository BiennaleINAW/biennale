<h1>Listing votes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Ip_adress:</th>
      <th>on:</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @votes.each do |vote| %>
      <tr>
        <td><%= vote.ip_address %> </td>
        <td><%= vote.contest_record.title unless vote.contest_record == nil %> </td>
        <td><%= link_to 'Show', [:admin, vote] %></td>
        <td><%= link_to 'Edit', edit_admin_vote_path(vote) %></td>
        <td><%= link_to 'Destroy', [:admin, vote], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote', new_admin_vote_path %>
